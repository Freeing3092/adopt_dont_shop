<% @admin_shelters.each do |shelter| %>
    <h1><%= shelter.name %></h1>
    <p>City: <%= shelter.city %></p>
    <p>Foster program: <%= shelter.foster_program %></p>
    <p>Created at: <%= shelter.created_at %></p>
    <p>Id: <%= shelter.id %></p>
<% end %>


<h2>Shelter's with Pending Applications</h2>

<% @pending_applications.each do |pending| %>
    
    <p> <%= pending.name %> </p>
<% end %>

